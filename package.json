{
  "name": "@architect-io/cli",
  "description": "Command-line interface for Architect.io",
  "version": "0.4.10-rc.4",
  "author": "Architect.io",
  "bin": {
    "architect": "./bin/run"
  },
  "bugs": "https://github.com/architect-team/architect-cli/issues",
  "dependencies": {
    "@oclif/command": "1.5.19",
    "@oclif/config": "1.14.0",
    "@oclif/plugin-help": "2.2.3",
    "@types/atob": "^2.1.2",
    "@types/btoa": "^1.2.3",
    "@types/is-ci": "^2.0.0",
    "@types/mustache": "^4.0.1",
    "@types/opener": "^1.4.0",
    "@types/shell-quote": "^1.6.2",
    "atob": "^2.1.2",
    "auth0": "2.25.1",
    "axios": "0.19.2",
    "base64url": "^3.0.1",
    "btoa": "^1.2.1",
    "class-transformer": "0.2.3",
    "class-validator": "0.12.2",
    "cli-table3": "0.5.1",
    "cli-ux": "5.3.2",
    "dotenv-expand": "5.1.0",
    "execa": "3.2.0",
    "fs-extra": "8.1.0",
    "inquirer": "7.1.0",
    "is-ci": "^2.0.0",
    "is-windows": "^1.0.2",
    "js-yaml": "3.13.1",
    "mustache": "^4.0.1",
    "opener": "^1.5.1",
    "p-limit": "^2.3.0",
    "reflect-metadata": "0.1.13",
    "shell-quote": "^1.7.2",
    "tslib": "1.10.0",
    "untildify": "4.0.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.22.2",
    "@oclif/test": "1.2.5",
    "@types/auth0": "2.20.4",
    "@types/axios": "0.14.0",
    "@types/chai": "4.2.4",
    "@types/fs-extra": "8.0.1",
    "@types/inquirer": "6.5.0",
    "@types/is-windows": "^1.0.0",
    "@types/js-yaml": "3.12.1",
    "@types/mocha": "7.0.2",
    "@types/mock-fs": "^4.10.0",
    "@types/moxios": "0.4.9",
    "@types/node": "10.17.0",
    "@types/sinon": "7.5.0",
    "@typescript-eslint/eslint-plugin": "2.23.0",
    "@typescript-eslint/parser": "2.23.0",
    "chai": "4.2.0",
    "chalk": "2.4.2",
    "copyfiles": "^2.3.0",
    "eslint": "6.8.0",
    "eslint-plugin-oclif": "0.1.0",
    "globby": "10.0.1",
    "mocha": "^7.2.0",
    "mock-fs": "^4.12.0",
    "mock-spawn": "0.2.6",
    "moxios": "0.4.0",
    "nyc": "15.0.0",
    "sinon": "7.5.0",
    "ts-node": "8.10.2",
    "typescript": "3.9.5"
  },
  "optionalDependencies": {
    "keytar": "4.13.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/architect-team/architect-cli",
  "keywords": [
    "oclif"
  ],
  "license": "GPL-3.0",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "architect",
    "dirname": "architect",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "hooks": {
      "init": "./lib/hooks/init/tty"
    },
    "macos": {
      "identifier": "io.architect.cli"
    }
  },
  "repository": "architect-team/architect-cli",
  "scripts": {
    "lint": "eslint --ext .ts,.js .",
    "fix": "eslint --ext .ts,.js . --fix",
    "build": "rm -rf ./lib && tsc -b && npm run copy-static",
    "copy-static": "copyfiles -u 1 \"src/static/*.html\" lib",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "coverage": "nyc --extension .ts npm test",
    "test": "mocha --forbid-only \"test/**/*.test.ts\" --config ./test/.mocharc.yml",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
