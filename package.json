{
  "name": "@architect-io/cli",
  "description": "Command line interface for creating and deploying architect services",
  "version": "0.2.4",
  "author": "David Thor @davidthor",
  "bin": {
    "architect": "./bin/run"
  },
  "bugs": "https://github.com/architect-team/architect-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.5.15",
    "@oclif/config": "^1.13.0",
    "@oclif/errors": "^1.2.2",
    "@oclif/plugin-autocomplete": "^0.1.3",
    "@oclif/plugin-commands": "^1.2.2",
    "@oclif/plugin-help": "^2.2.0",
    "@oclif/plugin-not-found": "^1.2.2",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "auth0": "^2.17.1",
    "axios": "^0.19.0",
    "chalk": "^2.4.2",
    "cli-ux": "^5.3.1",
    "dotenv": "^8.0.0",
    "execa": "^1.0.0",
    "fs-extra": "^8.0.1",
    "inquirer": "^6.4.1",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "is-ci": "^2.0.0",
    "joi": "^14.3.1",
    "js-yaml": "^3.13.1",
    "listr": "^0.14.3",
    "oclif": "^1.13.4",
    "protocol-buffers": "^4.1.0",
    "tslib": "^1.10.0",
    "untildify": "^4.0.0"
  },
  "optionalDependencies": {
    "keytar": "^4.13.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.0",
    "@oclif/test": "^1.2.4",
    "@oclif/tslint": "^3.1.0",
    "@types/chai": "^4.1.6",
    "@types/dotenv": "^6.1.1",
    "@types/execa": "^0.9.0",
    "@types/fs-extra": "^7.0.0",
    "@types/inquirer": "0.0.43",
    "@types/joi": "^14.3.3",
    "@types/js-yaml": "^3.12.1",
    "@types/listr": "^0.14.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.14.10",
    "chai": "^4.1.2",
    "globby": "^8.0.2",
    "mocha": "^5.2.0",
    "nock": "^10.0.6",
    "nyc": "^13.3.0",
    "sinon": "^6.3.5",
    "ts-node": "^7.0.1",
    "tslint": "^5.18.0",
    "typescript": "^3.5.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/architect-team/architect-cli",
  "keywords": [
    "oclif"
  ],
  "license": "GPL-3.0",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "architect",
    "plugins": [
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-commands",
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-warn-if-update-available"
    ]
  },
  "repository": "architect-team/architect-cli",
  "scripts": {
    "preinstall": "sh ./bin/preinstall.sh",
    "postinstall": "sh ./bin/postinstall.sh",
    "pretest": "tslint -p tsconfig.json -c tslint.json -t stylish",
    "test": "nyc --extension .ts mocha --exit --forbid-only \"test/**/*.test.ts\"",
    "build": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "version": "npm run build && git add -Af README.md",
    "postversion": "git push && git push --tags"
  },
  "types": "lib/index.d.ts"
}
